		"test": "ng test",
		"pretty": "prettier --check .",
		"pretty:fix": "prettier --write .",
		"lint": "eslint .",
		"e2e": "ng e2e",
		"prodtest": "ng build --prod --source-map && http-server-spa dist/MyMap/ index.html 8001",
		"deploy": "ng build --prod && firebase deploy --only hosting:main -m \"New release :)\"",
		"config": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" ./scripts/config.index.ts"
	},
	"repository": "github:HeleleF/mymap",
