{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/3rdpartylicenses.txt",
          "/index.html",
          "/*.css",
          "/*.js",
          "/*.json",
          "/assets/gyms/**",
          "/assets/quests/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "pogo-api",
      "urls": [
        "https://api.mapbox.com/**",
        "http://lh*.ggpht.com/**",
        "http://lh*.googleusercontent.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 500,
        "maxAge": "50d",
        "timeout": "7s"
      }
    }
  ]
}
